			</wsdl:output>
			<wsdl:fault name="SessionFault">
				<soap12:fault name="SessionFault" use="literal"/>
			</wsdl:fault>
		</wsdl:operation>
		<wsdl:operation name="RemovePublication">
			<soap12:operation soapAction="http://www.openoandm.org/isbm/ReadPublication" style="document"/>
			<wsdl:input>
				<soap12:body use="literal"/>
			</wsdl:input>
			<wsdl:output>
				<soap12:body use="literal"/>
			</wsdl:output>
			<wsdl:fault name="SessionFault">
				<soap12:fault name="SessionFault" use="literal"/>
			</wsdl:fault>
		</wsdl:operation>
		<wsdl:operation name="CloseSubscriptionSession">
			<soap12:operation soapAction="http://www.openoandm.org/isbm/CloseSubscriptionSession" style="document"/>
			<wsdl:input>
				<soap12:body use="literal"/>
			</wsdl:input>
			<wsdl:output>
				<soap12:body use="literal"/>
			</wsdl:output>
			<wsdl:fault name="SessionFault">
				<soap12:fault name="SessionFault" use="literal"/>
			</wsdl:fault>
		</wsdl:operation>
	</wsdl:binding>

	<wsdl:service name="ConsumerPublicationService">
		<wsdl:port name="ConsumerPublicationServiceSoap" binding="isbm:ConsumerPublicationServiceSoap">
			<soap:address location="https://example.com/ConsumerPublicationService"/>
		</wsdl:port>
		<wsdl:port name="ConsumerPublicationServiceSoap12" binding="isbm:ConsumerPublicationServiceSoap12">
			<soap12:address location="https://example.com/ConsumerPublicationService12"/>
		</wsdl:port>
	</wsdl:service>
</wsdl:definitions>
